function intfontsize(){
	
	
	var par = document.getElementById("para1");
	
	par.setAttribute('class','fontsizeup')
	
}

function addClickEvent(){
	
	var goToGoogleButton = document.getElementById("incbtn");
	
	goToGoogleButton.addEventListener('click', usingWindowObj);
	
	var img = document.getElementById("increment");
	
	img.addEventListener('click', intfontsize);
	
	
}

(function(){
	
	addClickEvent();
	
	
})();